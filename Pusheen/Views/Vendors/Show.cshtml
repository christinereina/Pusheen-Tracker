@using PusheenTracker.Models; 

<h3>Order List</h3>

<h3>Here are all the orders:</h3>
  @if (@Model["orders"].Count == 0)
  {
    <h3>No orders have been added yet.</h3>
  }
<ol>
@foreach (Order order in @Model["orders"])
  {
    <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'> @order.Type </a></li>
  }
  </ol>

<p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add a new Order</a></p>
<p><a href="/">Back home</a></p>
<p><a href="/vendors">Back to Vendors</a></p>